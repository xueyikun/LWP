id) {\n      this.$router.push({ name: \"singerDetail\", params: { id } });\n    },\n  },\n  created() {\n    this.getSearchSinger();\n  },\n};\n</script>\n\n<style scoped>\n.searchSinger {\n  max-width: 1000px;\n  margin: auto;\n}\n\n.tip {\n  font-weight: bold;\n  color: rgb(34, 34, 34);\n  margin-bottom: 10px;\n}\n\n.page {\n  width: 100%;\n  text-align: center;\n  padding-bottom: 20px;\n  margin: 20px 0;\n}\n</style>"],"sourceRoot":"src/views/search/searchChildren"}]}