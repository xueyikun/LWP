      this.userName = res.data.profile.nickname;\n    },\n\n    // 用户列表触底的回调\n    bottomLoad() {\n      // console.log(\"触底了\");\n      this.currentPage += 1;\n      this.getFolloweds();\n    },\n  },\n  created() {\n    this.getFolloweds();\n    this.getUserInfo();\n  },\n};\n</script>\n\n<style scoped>\n.Follows {\n  overflow-y: scroll;\n}\n\n.title {\n  color: #373737;\n  font-weight: bold;\n  margin: 20px 30px;\n}\n</style>\n"],"sourceRoot":"src/views/personal/personalChildren"}]}